<ModOps>
    <!-- Description -->
    <ImyaTweaks Title="Change some values from the TrainStation Warehouse Harbor." Description="You can Increase or Decrease the Max Storage,the Buff Radius and the Attractiveness Points" />
   
   <!-- Change Max Storage  -->
    <ModOp Type="replace" GUID="1337504862" Path="/Values/Warehouse/WarehouseStorage/StorageMax" ModOpID="OldTrainstationHarbor">
        <StorageMax>100</StorageMax>
    </ModOp>
	
    <ImyaExpose Path="self::StorageMax" ModOpID="OldTrainstationHarbor" ExposeID="OldTrainstationStorageHarbor" Description="The Old Trainstation Warehouse Harbor max Storage" Kind="Slider">
        <SliderDefinition Min="0" Max="5000" Stepping="10" />
    </ImyaExpose>
   
   <!-- Change Buff Radius  -->
    <ModOp GUID="1337504862" Type="replace" Path="/Values/ItemContainer/SocketScopeRadius" ModOpID="OldTrainstationRadiusHarbor">
        <SocketScopeRadius>50</SocketScopeRadius>
    </ModOp>
	
    <ImyaExpose Path="self::SocketScopeRadius" ModOpID="OldTrainstationRadiusHarbor" ExposeID="OldTrainstationHarborRadiusValue" Description="The Old Trainstation Warehouse Harbor Radius Value" Kind="Slider">
        <SliderDefinition Min="0" Max="150" Stepping="5" />
    </ImyaExpose>
   
   <!-- Change Attractiveness Points  -->
    <ModOp GUID="1337504862" Type="replace" Path="/Values/Culture/Attractiveness" ModOpID="OldTrainstationHarborAttractiveness">
        <Attractiveness>230</Attractiveness>
    </ModOp>
	
    <ImyaExpose Path="self::Attractiveness" ModOpID="OldTrainstationHarborAttractiveness" ExposeID="OldTrainstationHarborAttractivenessValue" Description="The Old Trainstation Warehouse Harbor Attractiveness Value" Kind="Slider">
        <SliderDefinition Min="0" Max="1000" Stepping="5" />
    </ImyaExpose>
</ModOps>

