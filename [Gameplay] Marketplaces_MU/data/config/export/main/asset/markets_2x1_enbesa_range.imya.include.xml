<ModOps>
  <!-- Description -->
  <ImyaTweaks Title="Change the Range of all ENBESA 2x1 Markets." Description="You can Increase or Decrease the Ranges of all ENBESA 2x1 Markets on Land" />
  
  <!-- Change FullSatisfactionDistance of 2x1 Markets ENBESA-->
  <ModOp Type="replace" GUID="1337502977" Path="/Values/PublicService/FullSatisfactionDistance" ModOpID="FullSatisfactionDistance2x1AllMarketsENBESAMU">
    <FullSatisfactionDistance>18</FullSatisfactionDistance>
  </ModOp>
  <ImyaExpose Path="self::FullSatisfactionDistance" ModOpID="FullSatisfactionDistance2x1AllMarketsENBESAMU" ExposeID="Custom FullSatisfactionDistance for 2x1 Markets ENBESA" Description="Sets the FullSatisfactionDistance for the 2x1 Markets in the ENBESA ( Mod default settings = 18 )" Kind="Slider">
    <SliderDefinition Min="0" Max="50" Stepping="1" />
  </ImyaExpose>
  
  <!-- Change NoSatisfactionDistance of 2x1 Markets ENBESA-->
  <ModOp Type="replace" GUID="1337502977" Path="/Values/PublicService/NoSatisfactionDistance" ModOpID="NoSatisfactionDistance2x1AllMarketsENBESAMU">
    <NoSatisfactionDistance>30</NoSatisfactionDistance>
  </ModOp>
  <ImyaExpose Path="self::NoSatisfactionDistance" ModOpID="NoSatisfactionDistance2x1AllMarketsENBESAMU" ExposeID="Custom NoSatisfactionDistance for 2x1 Markets ENBESA" Description="Sets the NoSatisfactionDistance for the 2x1 Markets in the ENBESA ( Mod default settings = 30 )" Kind="Slider">
    <SliderDefinition Min="0" Max="100" Stepping="1" />
  </ImyaExpose>
</ModOps>